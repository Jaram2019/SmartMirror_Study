    <!-- Content Wrapper -->
    <div id="content-wrapper" class="d-flex flex-column">

      <!-- Main Content -->
      <div id="content">
          <ul class="list-group">
              <% for(var i=0; i<eventList.length; i++){ %>
                <% var event = eventList[i]; %>
                <% var start = event.start.dateTime.split('+')[0].replace("T", " "); %>
                <% var end = event.end.dateTime.split('+')[0].replace("T", " "); %>
                
                <% var week = new Array('일요일', '월요일', '화요일', '수요일', '목요일', '금요일', '토요일'); %>
                <% var startDay = new Date(start.split(' ')[0]).getDay(); %>
                <% var endDay = new Date(end.split(' ')[0]).getDay(); %>
                <% var startDayLabel = week[startDay]; %>
                <% var endDayLabel = week[endDay]; %>

                <li class="list-group-item"><%= start %> - <%= startDayLabel %> - <%= end %> - <%= endDayLabel %> - <%= event.summary %></li>
              <% } %>
          </ul>
      </div>
      <!-- End of Main Content -->

